# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/db/*.sqlite3
/db/*.sqlite3-journal

/node_modules
/yarn-error.log

.byebug_history

.elasticbeanstalk/*
!.elasticbeanstalk/*.cfg.yml
!.elasticbeanstalk/*.global.yml

# SimpleCov reports
coverage/
